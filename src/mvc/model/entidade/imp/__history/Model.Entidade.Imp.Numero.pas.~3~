unit Model.Entidade.Imp.Numero;

interface

uses
  Model.Entidade.Numero.Interfaces;

type
  TEntidadeNumero<T> = class(TInterfacedObject, iEntidadeNumero<T>)
    private
      [weak]
      FParent      : T;
      FId          : Integer;
      FIdEndereco  : Integer;
      FNumero      : String;
      FComplemento : String;
    public
      constructor Create(Parent : T);
      destructor Destroy; override;
      class function New(Parent : T) : iEntidadeNumero<T>;

      function Id(Value : Integer)         : iEntidadeNumero<T>; overload;
      function Id                          : Integer;            overload;
      function IdEndereco(Value : Integer) : iEntidadeNumero<T>; overload;
      function IdEndereco                  : Integer;            overload;
      function Numero(Value : String)      : iEntidadeNumero<T>; overload;
      function Numero                      : String;             overload;
      function Complemento(Value : String) : iEntidadeNumero<T>; overload;
      function Complemento                 : String;             overload;

      function &End : T;
  end;

implementation

end.

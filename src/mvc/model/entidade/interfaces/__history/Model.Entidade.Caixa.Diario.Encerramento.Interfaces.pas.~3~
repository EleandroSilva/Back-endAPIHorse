unit Model.Entidade.Caixa.Diario.Encerramento.Interfaces;

interface

type
  iCaixaDiarioEncerramento<T> = interface
    ['{7895FB5B-D1A2-4CEF-9041-0732D2CD8558}']
    function Id              (Value : Integer) : iCaixaDiarioEncerramento<T>; overload;
    function Id                                : Integer;                     overload;
    function IdCaixaDiario   (Value : Integer) : iCaixaDiarioEncerramento<T>; overload;
    function IdCaixaDiario                     : Integer;                     overload;
    function IdUsuario       (Value : Integer) : iCaixaDiarioEncerramento<T>; overload;
    function IdUsuario                         : Integer;                     overload;
    function ValorLancamento (Value : Currency): iCaixaDiarioEncerramento<T>; overload;
    function ValorLancamento                   : Currency;                    overload;
    function DataHoraEmissao (TDateTime)       : iCaixaDiarioEncerramento<T>; overload;
    function DataHoraEmissao                   : TDateTime;                   overload;
    function Observacao       (Value : String) : iCaixaDiarioEncerramento<T>; overload;
    function Observacao                        : String;                      overload;

    function &End : T;
  end;

implementation

end.

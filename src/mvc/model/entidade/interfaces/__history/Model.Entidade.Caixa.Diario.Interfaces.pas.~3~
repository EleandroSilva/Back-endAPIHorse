unit Model.Entidade.Caixa.Diario.Interfaces;

interface

type
  iCaixaDiario<T> = interface
    ['{20798A26-E449-405D-9658-06C0034CAC46}']
    function Id(Value : Integer)                : iCaixaDiario<T>; overload;
    function Id                                 : Integer;         overload;
    function IdEmpresa(Valeu : Integer)         : iCaixaDiario<T>; overload;
    function IdEmpresa                          : Integer;         overload;
    function IdUsuario(Value : Integer)         : iCaixaDiario<T>; overload;
    function IdUsuario                          : Integer;         overload;
    function ValorInicial(Value : Currency)     : iCaixaDiario<T>; overload;
    function ValorInicial                       : Currency;        overload;
    function DataHoraEmissao(Value : TDateTime) : iCaixaDiario<T>; overload;
    function DataHoraEmissao                    : TDateTime;       overload;
    function Status(Value : String)             : iCaixaDiario<T>; overload;
    function Status                             : String;          overload;

    function &End : T;
  end;


implementation

end.

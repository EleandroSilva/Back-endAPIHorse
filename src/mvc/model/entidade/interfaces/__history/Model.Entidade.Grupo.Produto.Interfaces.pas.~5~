unit Model.Entidade.Grupo.Produto.Interfaces;

interface

type
  iGrupoProduto<T> = interface
    ['{8EAA1FED-0229-492C-B7EB-1FCC725B9C7D}']
    function Id(Value : Integer)        : iGrupoProduto<T>; overload;
    function Id                         : Integer;          overload;
    function IdEmpresa(Value : Integer) : iGrupoProduto<T>; overload;
    function IdEmpresa                  : Integer;          overload;
    function Nome(Value : String)       : iGrupoProduto<T>; overload;
    function Nome                       : String;           overload;
    function Ativo(Value : Integer)     : iGrupoProduto<T>; overload;
    function Ativo                      : Integer;          overload;

    function &End : T;
  end;

implementation

end.

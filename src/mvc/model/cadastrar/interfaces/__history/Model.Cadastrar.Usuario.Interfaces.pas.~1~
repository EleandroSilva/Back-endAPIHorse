interface

uses
  System.JSON,

  Model.Entidade.Usuario.Interfaces;

type
  iCadastrarUsuario = interface
    ['{918870CF-D295-4DC9-80D1-4E825CF4BBC6}']
    function JSONArrayUsuario (Value : TJSONArray)  : iCadastrarUsuario; overload;
    function JSONArrayUsuario                       : TJSONArray;        overload;
    function JSONObjectUsuario(Value : TJSONObject) : iCadastrarUsuario; overload;
    function JSONObjectUsuario                      : TJSONObject;       overload;

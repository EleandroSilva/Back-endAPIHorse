      function Error : Boolean;
      //injeção de dependência
      function EmailPessoa : iEntidadeEmailPessoa<iCadastrarEmailPessoa>;
      function &End : iCadastrarEmailPessoa;
  end;

implementation

uses
  Imp.Controller,
  Model.Entidade.Imp.Email.Pessoa;

{ TCadastrarEmailPessoa }


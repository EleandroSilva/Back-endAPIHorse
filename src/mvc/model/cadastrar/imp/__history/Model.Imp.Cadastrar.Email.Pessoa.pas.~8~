  Value: TJSONObject): iCadastrarEmailPessoa;
begin

end;

class function TCadastrarEmailPessoa.New: iCadastrarEmailPessoa;
begin

end;

function TCadastrarEmailPessoa.Post: iCadastrarEmailPessoa;

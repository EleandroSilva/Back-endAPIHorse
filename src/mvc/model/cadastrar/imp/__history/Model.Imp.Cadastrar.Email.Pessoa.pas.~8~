begin
  FController    := TController.New;
  FEmailPessoa   := TEntidadeEmailPessoa<iCadastrarEmailPessoa>.New(Self);
  FDSEmailPessoa := TDataSource;

  FError := False;
  FQuantidadeRegistro := 0;
end;


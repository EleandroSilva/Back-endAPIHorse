unit Model.Pequisar.Caixa.Interfaces;

interface

uses
  Data.DB;

type
  iPesquisarCaixa = Interface
    ['{FF9B4F5E-DFC9-4F10-8DB6-01DD0F8D9BC6}']
    function DataSet(Value : TDataSource) : iPesquisarCaixa; overload;
    function DataSet                      : TDataSource;     overload;
    function GetbyId(Id : Variant)        : iPesquisarCaixa; overload;
    function GetbyId(IdEmpresa : String)  : iPesquisarCaixa; overload;
    function GetbyParams(Value : String)  : iPesquisarCaixa;
    function GetAll                       : iPesquisarCaixa;
    function LoopCaixa                    : iPesquisarCaixa;
    function Found : Boolean;
    function Error : Boolean;
    function Caixa : iEntidadeCaixa<iPesquisarCaixa>;
    function &End  : iPesquisarCaixa;
  End;

implementation

end.

unit Tabela.Swagger.Municipio;

interface

type
  TMunicipio = class
    private
      FId        : Integer;{bigint-Primary Key}
      FIBGE      : String; {Varchar(18)->Unique Key-Not Null}
      FIdEstado  : String; {Varchar(20)->Caso não existir informar (ISENTO) em maiúsculo}
      FMunicipio : String; {Varchar(100)-> Not Null}
      FUF        : String; {Varchar(100)}
    public
      property Id                    : Integer read FId                    write FId;
      property IBGE                  : String  read FCNPJ                  write FCNPJ;
      property IdEstado     : String  read FInscricaoEstadual     write FInscricaoEstadual;
      property Municipio       : String  read FNomeEmpresarial       write FNomeEmpresarial;
      property UF          : String  read FNomeFantasia          write FNomeFantasia;
  end;


implementation

end.

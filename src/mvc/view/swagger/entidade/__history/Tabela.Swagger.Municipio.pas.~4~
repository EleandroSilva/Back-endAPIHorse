unit Tabela.Swagger.Municipio;

interface

type
  TMunicipio = class
    private
      FId        : Integer;{bigint-Primary Key}
      FIBGE      : Integer;{Integer)->Unique Key-Not Null}
      FIdEstado  : Integer;{Integer}
      FMunicipio : String; {Varchar(120)-> Not Null}
      FUF        : String; {Varchar(2)}
    public
      property Id        : Integer read FId                    write FId;
      property IBGE      : Integer read FIBGE                  write FCNPJ;
      property IdEstado  : Integer read FIdEstado     write FInscricaoEstadual;
      property Municipio : String  read FMunicipio       write FNomeEmpresarial;
      property UF        : String  read FUF          write FNomeFantasia;
  end;


implementation

end.

unit Tabela.Swagger.Pedido.Pagamento;

interface

uses
  GBSwagger.Model.Attributes;

type
  TPedidoPagamento = class
    private
      FId          : Integer;
      FIdPedido    : Integer;
      FIdPagamento : Integer;
      FDataVencimento : TDateTime;
      FValorParcela   : Currency;
    public
      [SwagIgnori]
      property id             : Integer   read FID             write FId;
      [SwagPro,True]
      property idpedido       : Integer   read FIdPedido       write FIdPedido;
      [SwagPro,True]
      property idpagamento    : Integer   read FIdPagamento    write FIdPagamento;
      [SwagPro,True]
      property datavencimento : TDateTime read FDataVencimento write FDataVencimento;
      [SwagPro,True]
      property valorparcela   : Currency  read FValorParcela   write FValorParcela;
  end;

implementation

end.

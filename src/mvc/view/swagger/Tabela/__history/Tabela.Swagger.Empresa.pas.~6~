unit Tabela.Swagger.Empresa;

interface

type
  TEmpresa = class
    private
      FId                : Integer;{bigint}
      FCNPJ              : String;{Varchar(18)}
      FInscricaoEstadual : String; {Varchar(20)}
      FNomeEmpresarial   : String;{Varchar(120)}
      FNomeFantasia      : String;{Varchar(100)}
      FSenha             : String;{Varchar(20)}
      FAtivo             : Integer;{Integer}
    public
      property Id                : Integer read FId                write FId;
      property CNPJ              : String  read FCNPJ              write FCNPJ;
      property InscricaoEstadual : String  read FInscricaoEstadual write FInscricaoEstadual;
      property NomeEmpresarial   : String  read FNomeEmpresarial   write FNomeEmpresarial;
      property NomeFantasia      : String  read FNomeFantasia      write FNomeFantasia;
      property Senha             : String  read FSenha             write FSenha;
      property Ativo             : Integer read FAtivo             write FAtivo;
  end;


implementation

end.

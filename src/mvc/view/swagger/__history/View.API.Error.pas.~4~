unit View.API.Error;

interface

type
  TAPIError = class
    private
      FErrorMessage : String;
      FDetails      : string;
      FErrorCode    : Integer;
    public
      property ErrorCode    : Integer read FErrorCode    write FErrorCode;
      property ErrorMessage : string  read FErrorMessage write FErrorMessage;
      property Details      : string  read FDetails      write FDetails;
  end;

implementation

end.
